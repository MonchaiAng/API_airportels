{
  "scripts": {
    "start": "cross-env NODE_ENV=development node src/server.js",
    "dev": "cross-env NODE_ENV=development nodemon src/server.js",
    "migrate": "sequelize --options-path=src/config/sequelize-options.js db:migrate",
    "undo-migrate": "sequelize --options-path=src/config/sequelize-options.js db:migrate:undo:all",
    "seed": "sequelize --options-path=src/config/sequelize-options.js db:seed:all",
    "reset-db": "npm run undo-migrate && npm run migrate && npm run seed",
    "reset-db-production": "cross-env NODE_ENV=production npm run undo-migrate && npm run migrate && npm run seed",
    "deploy": "git reset --hard && git pull origin develop && pm2 restart 4 && pm2 logs"
  },
